# Repository: CloudAnalytics

import pandas as pd
import matplotlib.pyplot as plt

# Load data from a CSV file
data = pd.read_csv('data.csv')

# Perform data analysis and visualization
# ...

# Generate a bar plot of a specific column
column_name = 'Sales'
plt.bar(data['Month'], data[column_name])
plt.xlabel('Month')
plt.ylabel(column_name)
plt.title(f'{column_name} by Month')
plt.show()
